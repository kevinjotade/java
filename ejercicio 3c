import java.util.Scanner;

/**
 *
 * @author LA FE
 */
    public class Tarea {

    public static void main(String[] args) {
        // Ingreso del valor de n
        Scanner sc = new Scanner(System.in);
        System.out.print("Ingrese el valor de n: ");
        int n = sc.nextInt();

        // Cálculo de la suma de cubos
        long sumaCubos = (long) (Math.pow(n, 2) * Math.pow(n + 1, 2)) / 4;

        // Impresión del resultado
        System.out.println("La suma de cubos de los " + n + " primeros números naturales es: " + sumaCubos);
    }
}
